# Mod key
set $mod Mod4

# Movement
set $left h
set $down j
set $up k
set $right l

# Terminal
set $term foot

# Browser
set $browser firefox

# File Manager
set $files nemo

# App menu
set $menu wofi

# GTK interface
set $gnome-schema org.gnome.desktop.interface

# Locker
set $locker "swaylock"

# Screenshot tool
set $printscreen 'grim -g "$(slurp)" - | swappy -f -'

# Exit
set $exit "wlogout -b 3" 

# Workspaces
set $ws1 1
set $ws2 2
set $ws3 3
set $ws4 4
set $ws5 5
set $ws6 6
set $ws7 7
set $ws8 8
set $ws9 9
set $ws10 0
